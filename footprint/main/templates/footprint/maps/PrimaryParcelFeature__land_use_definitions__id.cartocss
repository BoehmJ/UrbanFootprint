
[zoom<=13] {line-width:0;}
[zoom=14] {line-width:0.2;}
[zoom=15] {line-width:0.7;}
[zoom=16] {line-width:1;}
[zoom=17] {line-width:1.3;}
[zoom=18] {line-width:2.4;}
[zoom=19] {line-width:2.4;}
[zoom>=20] {line-width:3;}

